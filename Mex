#include<bits/stdc++.h>
using namespace std;
const int N = 2010;
bool b[N];
int n; 
int main()
{
    cin>>n;
    for(int i=0;i<n;i++)
    {
        int x;
        cin>>x;
        b[x]=true;
    }
    for(int i=0;i<=2000;i++)
    {
        if(b[i]==false)
        {
            cout<<i<<endl;
            break;
        }
    }
    return 0;
}
