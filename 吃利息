#include<bits/stdc++.h>
#define ios ios::sync_with_stdio(false),cin.tie(0),cout.tie(0);
typedef long long ll;
typedef unsigned long long ull;
using namespace std;
const int mod=1000000007;
const double eps=1e-8;
ll n,k;
ll ans;
void res(){
for(int i = 0 ; i < k ; i ++ )
	{
		ll tmp = n;
		if(n >= 50)	n += 5;
		else n += tmp / 10;
		n += 5;
	}
}
int main(){
	cin>>n>>k;
	ans = n;
	res();
	cout<<n<<"\n";
	return 0;
}
